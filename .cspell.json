{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "allowCompoundWords": true,
  "caseSensitive": false,
  "dictionaries": ["@flex-development/decorator-regex"],
  "dictionaryDefinitions": [
    {
      "name": "@flex-development/decorator-regex",
      "path": "./.dictionary.txt"
    }
  ],
  "enableGlobDot": true,
  "failFast": false,
  "flagWords": [],
  "ignorePaths": [
    "**/*.snap",
    "**/.eslintignore",
    "**/.gitconfig",
    "**/.gitignore",
    "**/.prettierignore",
    "**/CHANGELOG.md",
    "**/LICENSE.md",
    "**/RELEASE_NOTES.md",
    ".cspell.json",
    ".git/",
    ".husky/_/",
    ".vscode/settings.json",
    ".yarn/",
    "patches/",
    "yarn.lock"
  ],
  "ignoreRegExpList": [
    "/@flex-development\\/.*/",
    "/from\\s+(['\"]).*\\1/",
    "import\\(.*\\)"
  ],
  "ignoreWords": [],
  "language": "en-US",
  "patterns": [],
  "readonly": true,
  "useGitignore": true,
  "usePnP": false,
  "version": "0.2",
  "words": []
}
